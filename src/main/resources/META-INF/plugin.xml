<idea-plugin>
    <id>de.jaimerojas.clickup</id>
    <name>ClickUp</name>
    <version>0.0.3</version>
    <vendor email="jaimitorojas@gmail.com">Jaime Rojas</vendor>

    <description>
        Integrate ClickUp with JetBrains IDEs for seamless task management and time tracking. Track time spent on
        tasks and update them directly in ClickUp.
    </description>

    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.tasks</depends>
    <actions>
        <!-- Add your actions here -->
    </actions>
    <extensions defaultExtensionNs="com.intellij">
        <tasks.repositoryType implementation="de.jaimerojas.clickup.ClickUpRepositoryType"/>
    </extensions>
</idea-plugin>